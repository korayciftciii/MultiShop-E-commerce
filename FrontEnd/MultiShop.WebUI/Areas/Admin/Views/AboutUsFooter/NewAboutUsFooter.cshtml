@model CreateAboutUsFooterDto
<section class="content-header">
    <h1>Create Content</h1>
    <ol class="breadcrumb">
        <li><a asp-controller="Home" asp-action="Index"><i class="fa fa-home"></i> Home</a></li>
        <li><a asp-controller="AboutUsFooter" asp-action="Index"><i class="fa fa-home"></i> About Us Footer</a></li>
        <li class="active"><i class="fa fa-bars"></i> New Content</li>
    </ol>
</section>



<div class="col-md-12">
    <div class="chart-box">
        <h4>New Content</h4>
        <div class="row">
            <form asp-action="NewAboutUsFooter" method="post">
                <div class="col-md-4">
                    <fieldset class="form-group">
                        <label asp-for="Title">Title</label>
                        <input class="form-control" asp-for="Title" type="text">
                    </fieldset>
                </div>
                <div class="col-md-4">
                    <fieldset class="form-group">
                        <label asp-for="ShortDescription">Short Description</label>
                        <input class="form-control" asp-for="ShortDescription" type="text">
                    </fieldset>
                </div>

                <div class="col-md-4">
                    <fieldset class="form-group">
                        <label asp-for="CompanyEmail">Email</label>
                        <input class="form-control" asp-for="CompanyEmail" type="email">
                    </fieldset>
                </div>
                <div class="col-md-4">
                    <fieldset class="form-group">
                        <label asp-for="CompanyPhoneNumber">Phone Number</label>
                        <input class="form-control" asp-for="CompanyPhoneNumber" type="text">
                    </fieldset>
                </div>
                <div class="col-md-4">
                    <fieldset class="form-group">
                        <label asp-for="Address">Address</label>
                        <input class="form-control" asp-for="Address" type="text">
                    </fieldset>
                </div>

                <button type="submit" class="btn btn-default">Submit</button>
            </form>
        </div>
    </div>
</div>

